{
  "name": "app0",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/app0/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rspack:rspack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "web",
        "outputPath": "dist/apps/app0",
        "index": "apps/app0/src/index.html",
        "main": "apps/app0/src/main.ts",
        "tsConfig": "apps/app0/tsconfig.app.json",
        "rspackConfig": "apps/app0/rspack.config.ts",
        "assets": ["apps/app0/src/favicon.ico", "apps/app0/src/assets"]
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production",
          "optimization": true,
          "sourceMap": false
        }
      }
    },
    "serve": {
      "executor": "@nx/rspack:module-federation-dev-server",
      "options": {
        "buildTarget": "app0:build:development",
        "port": 4201
      },
      "configurations": {
        "development": {},
        "production": {
          "buildTarget": "app0:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "serve-static": {
      "executor": "@nx/rspack:module-federation-static-server",
      "defaultConfiguration": "production",
      "options": {
        "serveTarget": "app0:serve"
      },
      "configurations": {
        "development": {
          "serveTarget": "app0:serve:development"
        },
        "production": {
          "serveTarget": "app0:serve:production"
        }
      }
    }
  }
}
