{
  "name": "@mf-benchmark/app0",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "app0",
    "projectType": "application",
    "sourceRoot": "apps/app0/src",
    "targets": {
      "build": {
        "executor": "@nx/rspack:rspack",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "target": "web",
          "outputPath": "dist/apps/app0",
          "index": "apps/app0/src/index.html",
          "main": "apps/app0/src/main.tsx",
          "tsConfig": "apps/app0/tsconfig.app.json",
          "rspackConfig": "apps/app0/rspack.config.js",
          "assets": [
            "apps/app0/src/favicon.ico",
            "apps/app0/src/assets"
          ]
        },
        "configurations": {
          "development": {
            "mode": "development"
          },
          "production": {
            "mode": "production",
            "optimization": true,
            "sourceMap": false
          }
        }
      },
      "serve": {
        "executor": "@nx/rspack:dev-server",
        "options": {
          "buildTarget": "app0:build:development"
        },
        "configurations": {
          "development": {},
          "production": {
            "buildTarget": "app0:build:production"
          }
        }
      },
      "serve-static": {
        "executor": "@nx/web:file-server",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "app0:build",
          "spa": true
        }
      }
    }
  },
  "dependencies": {
    "@mf-benchmark/lib0": "workspace:*",
    "@mf-benchmark/lib1": "workspace:*",
    "@mf-benchmark/lib2": "workspace:*"
  }
}
